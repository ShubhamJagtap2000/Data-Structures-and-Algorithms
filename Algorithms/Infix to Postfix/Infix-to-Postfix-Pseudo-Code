InfixToPostfix(exp)
{
  Create a Stack S
  res <- empty string
  for i <- 0 to length(exp) - 1
  {
    if exp[i] is operand
      res <- res + exp[i]
    else if exp[i] is perator
    {
      while(!s.empty() && HasHigherPrecedence(S.top(), exp[i]))
      {
        res <- res + S.top()
        S.pop()
      }
      S.push(exp[i])
    }
  }
  while(!S.empty())
  {
    res <- res + S.top()
    S.pop()
  }
  return res
}
